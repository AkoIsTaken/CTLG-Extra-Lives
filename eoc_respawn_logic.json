    "effect": [ { "u_message": "You have no extra lives left. You die...", "popup": true, "type": "bad" } ],
    "false_effect": [ { "run_eocs": [ "EOC_respawn_check_difficulty" ] } ]
  },
  {
    "type": "effect_on_condition",
    "id": "EOC_respawn_check_difficulty",
    "condition": { "math": [ "u_respawn_diff_selection", ">=", "0" ] },
    "effect": {
      "switch": { "math": [ "u_respawn_diff_selection" ] },
      "cases": [
        {
          "case": 0,
          "effect": [ { "u_message": "Case 0 selected.", "popup": true, "type": "bad" }, { "run_eocs": [ "EOC_respawn_choose_here" ] } ]
        },
        {
          "case": 1,
