[
  {
    "type": "effect_on_condition",
    "id": "EOC_respawn_begin",
    "eoc_type": "PREVENT_DEATH",
    "condition": { "or": [ { "math": [ "u_lives_remaining", ">", "0" ] }, { "math": [ "u_unlimited_lives", "==", "1" ] } ] },
    "effect": [ { "math": [ "u_death_count", "+=", "1" ] }, { "run_eocs": [ "EOC_respawn_check_difficulty" ] } ],
    "false_effect": [ { "math": [ "u_death_count", "+=", "1" ] }, { "run_eocs": [ "EOC_respawn_check_permadeath" ] } ]
  },
  {
    "type": "effect_on_condition",
    "id": "EOC_respawn_check_permadeath",
